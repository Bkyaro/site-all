{
  "manifest_version": 2,
  "name": "Quick Links",
  "version": "1.0",
  "description": "A simple Chrome extension for quick access to websites.",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo16.png",
      "48": "images/logo48.png",
      "128": "images/logo128.png"
    }
  },
  "permissions": ["storage"]
}
